@model FarmaciaWeb.Models.Salida

<h2>Registrar Salida</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label>ProductoId</label>
        <input asp-for="ProductoId" class="form-control" />
    </div>
    <div class="form-group">
        <label>Cantidad</label>
        <input asp-for="Cantidad" class="form-control" />
    </div>
    <button type="submit" class="btn btn-success mt-2">Registrar</button>
    <a asp-action="Index" class="btn btn-secondary mt-2">Volver</a>
</form>
